{
  "kind": "collectionType",
  "collectionName": "events",
  "info": {
    "singularName": "event",
    "pluralName": "events",
    "displayName": "Event",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "small_description": {
      "type": "text",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "title",
      "required": true
    },
    "background": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "gallery": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "speakers": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::speaker.speaker",
      "mappedBy": "event"
    },
    "schedules": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::schedule.schedule",
      "mappedBy": "event"
    },
    "full_description": {
      "type": "blocks"
    },
    "event_type": {
      "type": "enumeration",
      "enum": [
        "ЮсУрал",
        "ТинсУрал",
        "БизнесКонференция"
      ]
    },
    "register_persons": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::register-person.register-person",
      "mappedBy": "events"
    }
  }
}
