{
  "kind": "collectionType",
  "collectionName": "events",
  "info": {
    "singularName": "event",
    "pluralName": "events",
    "displayName": "Event",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "small_description": {
      "type": "text",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "title",
      "required": true
    },
    "background": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "gallery": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "reg_price": {
      "type": "integer"
    },
    "speakers": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::speaker.speaker",
      "mappedBy": "event"
    },
    "increase_date": {
      "type": "string",
      "required": false
    },
    "increase_price": {
      "required": false,
      "type": "float"
    },
    "event_type": {
      "type": "string",
      "required": true,
      "default": "default"
    },
    "schedules": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::schedule.schedule",
      "mappedBy": "event"
    },
    "full_description": {
      "type": "blocks"
    }
  }
}
